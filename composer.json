{
    "name": "meetingdoctors/shared",
    "description": "This is a project providing infrastructure and testing helpers for creating CQRS and event sourced applications.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "beberlei/assert": "^3.3",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.49",
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^1.10",
        "rector/rector": "^1.0",
        "phpstan/phpstan-symfony": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Shared\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shared\\Tests\\": "tests/"
        }
    }
}
